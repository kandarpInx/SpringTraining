<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="shape" class="com.example.demo.Circle">
		<property name="name" value="Circle" />
	</bean>
	
	<bean name="shape1" class="com.example.demo.Square">
		<property name="name" value="Square" />
	</bean>
	
	<alias name="shape" alias="demo"/>
	<alias name="shape1" alias="shape"/>
	
</beans>
